<script>
    // Countdown
        countdown.setLabels(
        ' millisecond | Sek. | Min. | Stunde | Tag | Woche | Monat | Jahr| decade| century| millennium',
        ' milliseconds | Sek. | Min. | Stunden | Tage | Wochen | Monate | Jahre| decades| centuries| millennia',
        ' und ',
        ', ',
        'Jetzt',
        function(n){ return n.toString(); });

      function getRemainingTime(deadline,id) {
          // var now = new Date();
          var deadline = deadline;
          var id = '#'+id;
          var output;
          var outputDiv = document.querySelector(id);

          var t = Date.parse(deadline) - Date.parse(new Date());
          if (t > 0) {
            output = countdown(null, deadline, countdown.YEARS |	countdown.MONTHS | countdown.WEEKS | countdown.DAYS | countdown.HOURS | countdown.MINUTES, 1);
          } else {
            output = "Vorbei";
            outputDiv.previousElementSibling.innerHTML = '';
            outputDiv.nextElementSibling.innerHTML = '';
          }
          outputDiv.innerHTML = output;

      }
      </script>


